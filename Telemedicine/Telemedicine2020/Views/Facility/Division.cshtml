<!-- Edit Modal HTML -->
<div id="editEmployeeModal" class="modal fade view_update1">
    <div class="modal-dialog modal-md">
        <div class="modal-content center-block">
            <form>
                <div class="modal-header">
                    <h4 class="modal-title">View OR Update Facility Record</h4>
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                </div>
                <div class="modal-body">
                    <div class="form-group">
                        <div class="row">
                            <div class="col-md-6">
                                <label>District Name</label>
                                <input type="text" value="Raipur" class="form-control" required disabled>
                                <i data-feather="edit"></i>
                            </div>

                            <div class="col-md-6">
                                <div class="form-group">
                                    <label>Block/Tahsil</label>
                                    <select>
                                        <option selected="selected"></option>


                                    </select><i data-feather="edit"></i>

                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-6">
                            <div class="form-group">
                                <label>Town/Villege</label>
                                <input type="text" class="form-control" required>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label>Facility Type</label>
                                <input type="text" class="form-control" required>
                            </div>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-md-6">
                            <div class="form-group">
                                <label>Facility Name</label>
                                <input type="text" class="form-control" required>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label>Category</label>
                                <input type="text" class="form-control" required>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-6">
                            <div class="form-group">
                                <label>Location</label>
                                <input type="text" class="form-control" required>
                            </div>
                        </div>
                    </div>



                </div>
                <div class="modal-footer">
                    <input type="button" class="btn btn-default" data-dismiss="modal" value="Cancel">
                    <input type="submit" class="btn btn-info" value="Save">
                </div>
            </form>
        </div>
    </div>
</div>