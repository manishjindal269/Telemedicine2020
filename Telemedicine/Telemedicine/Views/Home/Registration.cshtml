
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

<!-- Mirrored from www.nobleui.com/html/template/demo_3/pages/auth/register.html by HTTrack Website Copier/3.x [XR&CO'2014], Mon, 04 May 2020 11:21:01 GMT -->
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Dashboard</title>
    <!-- core:css -->
    <link rel="stylesheet" href="~/Content/assets/vendors/core/core.css">
    <!-- endinject -->
    <!-- plugin css for this page -->
    <!-- end plugin css for this page -->
    <!-- inject:css -->
    <link rel="stylesheet" href="~/Content/assets/fonts/feather-font/css/iconfont.css">
    <link rel="stylesheet" href="~/Content/assets/vendors/flag-icon-css/css/flag-icon.min.css">
    <!-- endinject -->
    <!-- Layout styles -->
    <link rel="stylesheet" href="~/Content/assets/css/demo_1/style.css">
    <!-- End layout styles -->
    <link rel="shortcut icon" href="~/Content/assets/images/favicon.png" />
</head>
<body class="sidebar-dark">
    <div class="main-wrapper container" style="margin-top:50px;">
        <div class="page-wrapper full-page">
            @*<div class="page-content d-flex align-items-center justify-content-center">*@
            <div class="row">
                <div class="col-md-12 grid-margin stretch-card">
                    <div class="card">
                        <div class="card-body">
                            <h6 class="card-title">पंजीकरण फॉर्म</h6>
                            @using (Html.BeginForm("Index", "Home", FormMethod.Post))
                            {
                                <div class="row">
                                    <div class="col-md-1"></div>
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <input type="text" class="form-control" id="exampleInputText1" value="" placeholder="Enter Mobile Number...">
                                        </div>
                                    </div>
                                    <div class="col-md-2">
                                        <div class="form-group">
                                            <button class="btn btn-primary" type="button">Send OTP</button>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <input type="text" class="form-control" id="exampleInputText1" value="" placeholder="Enter OTP....">
                                        </div>
                                    </div>
                                    <div class="col-md-2">
                                        <div class="form-group">
                                            <button class="btn btn-primary" type="button" id="verify">Verify</button>
                                        </div>
                                    </div>
                                    <div class="col-md-1"></div>
                                </div>
                                <hr style="color:#F9FAFB; display:none;" id="hr" />
                                <div class="row" id="RegDet1" style="display:none;">
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            @*<label for="exampleInputText1">सदस्य का नाम</label>*@
                                            <input type="text" class="form-control" id="exampleInputText1" value="" placeholder="सदस्य का नाम">
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            @*<label for="exampleFormControlSelect1">जन्म तिथि</label>*@
                                            <div class="input-group date datepicker" id="datePickerExample">
                                                <input type="text" class="form-control"><span class="input-group-addon"><i data-feather="calendar"></i></span>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-md-4">
                                        <div class="form-group">
                                            @*<label for="exampleFormControlSelect1">लिंग</label>*@
                                            <select class="form-control" id="exampleFormControlSelect1">
                                                <option selected disabled>[-- लिंग चुनें --]</option>
                                                <option>पुरुष</option>
                                                <option>महिला</option>
                                                <option>अन्य</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>
                                <div class="row" id="RegDet2" style="display:none;">
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <div class="form-group">
                                                @*<label for="exampleInputText1">पासवर्ड</label>*@
                                                <input type="password" class="form-control" id="exampleInputText1" value="" placeholder="पासवर्ड">
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <div class="form-group">
                                                @*<label for="exampleInputText1">कंफ़र्म पासवर्ड</label>*@
                                                <input type="password" class="form-control" id="exampleInputText1" value="" placeholder="कंफ़र्म पासवर्ड">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row" id="RegDet3" style="display:none;">
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            @*<label for="exampleFormControlTextarea1">पता</label>*@
                                            <textarea class="form-control" id="exampleFormControlTextarea1" rows="5" placeholder="पता"></textarea>
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <div class="form-group">
                                                @*<label for="exampleInputText1">Latitude</label>*@
                                                <input type="password" class="form-control" id="exampleInputText1" value="" placeholder="Latitude">
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <div class="form-group">
                                                @*<label for="exampleInputText1">Longitude</label>*@
                                                <input type="password" class="form-control" id="exampleInputText1" value="" placeholder="Longitude">
                                            </div>
                                        </div>
                                    </div>

                                    <button type="submit" class="btn btn-primary mr-2">Submit</button>
                                    <button class="btn btn-light" type="reset">Reset</button>
                                </div>
                            }
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- core:js -->
    <script src="~/Content/assets/vendors/core/core.js"></script>
    <script src="~/Content/assets/vendors/bootstrap-datepicker/bootstrap-datepicker.min.js"></script>
    <!-- endinject -->
    <!-- plugin js for this page -->
    <!-- end plugin js for this page -->
    <!-- inject:js -->
    <script src="~/Content/assets/vendors/feather-icons/feather.min.js"></script>
    <script src="~/Content/assets/js/template.js"></script>
    <script src="~/Content/assets/js/datepicker.js"></script>
    <!-- endinject -->
    <!-- custom js for this page -->
    <!-- end custom js for this page -->
    <script type="text/javascript">
        $(document).ready(function () {
            $("#verify").click(function () {
                $("#RegDet1").show();
                $("#RegDet2").show();
                $("#RegDet3").show();
                $("#hr").show();
            });
        });
    </script>
</body>

